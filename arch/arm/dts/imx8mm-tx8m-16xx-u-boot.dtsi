#include "imx8mm-u-boot.dtsi"
#include "imx8mm-karo-u-boot.dtsi"

&binman {
	u-boot-spl-ddr {
		/delete-node/ blob-ext@3;

		/delete-node/ blob-ext@4;
	};
};

&blob_1d_imem {
	filename = "ddr3_imem_1d.bin";
};

&blob_1d_dmem {
	filename = "ddr3_dmem_1d.bin";
};

&pmic {
	regulators {
		BUCK1 {
			u-boot,dm-spl;
		};

		BUCK2 {
			u-boot,dm-spl;
		};

		BUCK3 {
			u-boot,dm-spl;
		};

		BUCK4 {
			u-boot,dm-spl;
		};

		BUCK5 {
			u-boot,dm-spl;
		};

		BUCK6 {
			u-boot,dm-spl;
		};

		BUCK7 {
			u-boot,dm-spl;
		};

		BUCK8 {
			u-boot,dm-spl;
		};

		LDO1 {
			u-boot,dm-spl;
		};

		LDO2 {
			u-boot,dm-spl;
		};

		LDO3 {
			u-boot,dm-spl;
		};

		LDO4 {
			u-boot,dm-spl;
		};

		LDO5 {
			u-boot,dm-spl;
		};

		LDO6 {
			u-boot,dm-spl;
		};

		LDO7 {
			u-boot,dm-spl;
		};
	};
};

&usbmisc1 {
	u-boot,dm-spl;
};

&usbotg1 {
	u-boot,dm-spl;
};

&usbphynop1 {
	u-boot,dm-spl;
};
